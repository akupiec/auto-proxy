{
  "server": {
  	"fallback": "index.html"
  },
  "proxies": [
	{
	  "contextPath": "/api",
	  "cache": {
		"enabled": true,
		"exclude": {
		  "paths": ["/files"],
		  "codes": [400, 403],
		  "headers": ["X-User-Name"],
		  "params": ["ignored_id"]
		},
		"include": {
		  "paths": ["/files_only"],
		  "codes": [200],
		  "headers": ["context"],
		  "params": ["only_id"]
		}
	  }
	}
  ]
}
